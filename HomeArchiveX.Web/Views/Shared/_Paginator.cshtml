@model HomeArchiveX.Web.Models.PaginatorViewModel
<div class="paginator">
@for (int i = 1; i < @Model.TotalPages + 1; i++)
{
    <div class="page-number">
        @if (i == @Model.CurrentPage) {
            @i
        }else
        {
            @Html.ActionLink(i.ToString(), "Index", new { page = i })
        }
        
    </div>
}
</div>
